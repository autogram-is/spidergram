{
  "name": "spidergram",
  "version": "0.6.0",
  "description": "Structural analysis tools for complex web sites",
  "exports": "./dist/source/index.js",
  "types": "./dist/source/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=14.20.0"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "nuke": "npm run clean; rm -rf ./node_modules; rm package-lock.json; npm install",
    "lint": "xo",
    "fix": "xo --fix",
    "compile": "tsc",
    "scratch": "ts-node-esm -T tests/fixtures/scratch.ts"
  },
  "files": [
    "dist/source/**/*"
  ],
  "xo": {
    "space": true,
    "bracketSpacing": true,
    "semicolon": true,
    "rules": {
      "new-cap": "off",
      "max-params": "off"
    },
    "envs": [
      "es2022",
      "node"
    ]
  },
  "author": "Jeff Eaton <jeff@autogram.is> (http://eaton.fyi)",
  "license": "MIT",
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/node": "^14.11.2",
    "@types/object-hash": "^2.2.1",
    "@types/uuid": "^8.3.4",
    "ava": "^4.3.1",
    "typescript": "^4.7.4",
    "ts-node": "^10.8.0",
    "xo": "^0.51.0"
  },
  "dependencies": {
    "@sindresorhus/is": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "crawlee": "^3.0.4",
    "dot-prop": "^7.2.0"
  }
}
