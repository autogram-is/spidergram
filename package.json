{
  "name": "spidergram",
  "version": "3.0.0",
  "description": "Structural analysis tools for complex web sites",
  "exports": "./dist/source/index.js",
  "types": "./dist/source/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "nuke": "npm run clean; rm -rf ./node_modules; rm package-lock.json; npm install",
    "test": "ava",
    "lint": "xo",
    "fix": "xo --fix",
    "compile": "tsc",
    "scratch": "ts-node-esm -T tests/fixtures/scratch.ts",
    "prepare": "npm run clean; npm run compile",
    "pretest": "npm run clean; npm run compile"
  },
  "files": [
    "dist/source/**/*"
  ],
  "xo": {
    "space": true,
    "bracketSpacing": true,
    "semicolon": true,
    "rules": {
      "new-cap": "off",
      "max-params": "off"
    },
    "envs": [
      "es2022",
      "node"
    ]
  },
  "ava": {
    "files": [
      "tests/**/*",
      "!tests/fixtures"
    ],
    "typescript": {
      "rewritePaths": {
        "tests/": "dist/tests/"
      },
      "compile": false
    }
  },
  "author": "Jeff Eaton <jeff@autogram.is> (http://eaton.fyi)",
  "license": "MIT",
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/cheerio": "^0.22.31",
    "@types/html-to-text": "^8.1.1",
    "@types/node": "^14.11.2",
    "@types/object-hash": "^2.2.1",
    "@types/uuid": "^8.3.4",
    "ava": "^4.3.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.4",
    "xo": "^0.51.0"
  },
  "dependencies": {
    "@autogram/url-tools": "^2.2.0",
    "@sindresorhus/is": "^5.3.0",
    "@sindresorhus/slugify": "^2.1.1",
    "@types/content-type": "^1.1.5",
    "@types/d3": "^7.4.0",
    "@types/jsdom": "^20.0.0",
    "@types/jsonfile": "^6.1.0",
    "@types/mime": "^3.0.1",
    "@types/minimatch": "^5.1.2",
    "@types/server-destroy": "^1.0.1",
    "arangojs": "^8.0.0-rc.1",
    "arrify": "^3.0.0",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "content-type": "^1.0.4",
    "crawlee": "^3.1.0",
    "d3": "^7.6.1",
    "dot-prop": "^7.2.0",
    "dotenv": "^16.0.3",
    "enquirer": "^2.3.6",
    "google-auth-library-nodejs": "^0.0.1-security.6",
    "googleapis": "^108.0.1",
    "html-to-text": "^8.2.1",
    "jsdom": "^20.0.1",
    "jsonfile": "^6.1.0",
    "listr2": "^5.0.5",
    "mime": "^3.0.0",
    "minimatch": "^5.1.0",
    "object-hash": "^3.0.0",
    "playwright": "^1.27.1",
    "readability-scores": "^1.0.8",
    "reflect-metadata": "^0.1.13",
    "server-destroy": "^1.0.1",
    "type-fest": "^3.1.0",
    "typefs": "^1.1.147",
    "uuid": "^9.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.18.12/xlsx-0.18.12.tgz"
  }
}
