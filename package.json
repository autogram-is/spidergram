{
  "name": "spidergram",
  "version": "0.6.0",
  "description": "Structural analysis tools for complex web sites",
  "exports": "./dist/source/index.js",
  "types": "./dist/source/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=14.20.0"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "nuke": "npm run clean; rm -rf ./node_modules; rm package-lock.json; npm install",
    "test": "ava",
    "lint": "xo",
    "fix": "xo --fix",
    "compile": "tsc",
    "scratch": "ts-node-esm -T tests/fixtures/scratch.ts",
    "prepare": "npm run clean; npm run compile",
    "pretest": "npm run clean; npm run compile"
  },
  "files": [
    "dist/source/**/*"
  ],
  "xo": {
    "space": true,
    "bracketSpacing": true,
    "semicolon": true,
    "rules": {
      "new-cap": "off",
      "max-params": "off"
    },
    "envs": [
      "es2022",
      "node"
    ]
  },
  "ava": {
    "files": [
      "tests/**/*",
      "!tests/fixtures"
    ],
    "typescript": {
      "rewritePaths": {
        "tests/": "dist/tests/"
      },
      "compile": false
    }
  },
  "author": "Jeff Eaton <jeff@autogram.is> (http://eaton.fyi)",
  "license": "MIT",
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/node": "^14.11.2",
    "@types/object-hash": "^2.2.1",
    "@types/uuid": "^8.3.4",
    "ava": "^4.3.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.4",
    "xo": "^0.51.0"
  },
  "dependencies": {
    "@autogram/url-tools": "^2.2.0",
    "@ava/typescript": "^3.0.1",
    "@sindresorhus/is": "^5.3.0",
    "@types/dotenv": "^8.2.0",
    "arangojs": "^7.8.0",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "crawlee": "^3.0.4",
    "dot-prop": "^7.2.0",
    "dotenv": "^16.0.3",
    "object-hash": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0"
  }
}
