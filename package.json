{
  "name": "spidergram",
  "version": "0.7.0-dev",
  "description": "Structural analysis tools for complex web sites",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^18.0.0"
  },
  "bin": {
    "spidergram": "./bin/run.js"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "nuke": "npm run clean; rm -rf ./node_modules; rm package-lock.json; npm install",
    "prepublish": "npm run build",
    "build": "npm run clean; npm run compile",
    "compile": "tsc",
    "scratch": "ts-node-esm src/examples/scratch.ts",
    "docs": "rm -rf ./docs/*; typedoc src/index.ts --out ./docs"
  },
  "files": [
    "dist/**/*",
    "bin"
  ],
  "author": {
    "name": "Autogram",
    "email": "team@autogram.is",
    "url": "https://autogram.is"
  },
  "contributors": [
    "Jeff Eaton <jeff@autogram.is> (http://eaton.fyi)",
    "Ethan Marcotte <karen@autogram.is> (https://ethanmarcotte.com)",
    "Karen McGrane <karen@autogram.is> (https://karenmcgrane.com)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/autogram-is/spidergram"
  },
  "bugs": {
    "url": "https://github.com/autogram-is/spidergram/issues"
  },
  "homepage": "https://spidergram.autogram.is",
  "oclif": {
    "bin": "spidergram",
    "dirname": "spidergram",
    "commands": "./dist/cli/commands",
    "topics": {
      "project": {
        "description": "Configure crawl projects"
      },
      "db": {
        "description": "Manage and query stored crawl data"
      }
    },
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@autogram/url-tools": "^2.5.2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.6",
    "@sindresorhus/is": "^5.3.0",
    "@sindresorhus/slugify": "^2.1.1",
    "@vladfrangu/async_event_emitter": "^2.1.2",
    "arrify": "^3.0.0",
    "chalk": "^5.1.2",
    "class-transformer": "^0.5.1",
    "cli-progress": "^3.11.2",
    "cli-table3": "^0.6.3",
    "content-type": "^1.0.4",
    "dot-prop": "^7.2.0",
    "dotenv": "^16.0.3",
    "filenamify": "^5.1.1",
    "humanize-url": "^3.0.0",
    "inquirer": "^9.1.4",
    "jsonld": "^8.1.0",
    "lodash": "^4.17.21",
    "log-symbols": "^5.1.0",
    "luxon": "^3.1.0",
    "mime": "^3.0.0",
    "minimatch": "^5.1.0",
    "object-hash": "^3.0.0",
    "open": "^8.4.0",
    "prepend-http": "^4.0.0",
    "protocolify": "^4.0.0",
    "read-pkg-up": "^9.1.0",
    "reflect-metadata": "^0.1.13",
    "schema-dts": "^1.1.0",
    "terminal-link": "^3.0.0",
    "type-fest": "^3.1.0",
    "typefs": "^1.1.147",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.8",
    "@types/cheerio": "^0.22.31",
    "@types/cli-progress": "^3.11.0",
    "@types/content-type": "^1.1.5",
    "@types/html-to-text": "^8.1.1",
    "@types/inquirer": "^9.0.3",
    "@types/jsdom": "^20.0.0",
    "@types/lodash": "^4.14.189",
    "@types/luxon": "^3.1.0",
    "@types/mime": "^3.0.1",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^18.11.0",
    "@types/object-hash": "^2.2.1",
    "@types/uuid": "^8.3.4",
    "@types/jsonld": "^1.5.8",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.8.0",
    "typedoc": "^0.23.19",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@oclif/core": "^1.19.1",
    "arangojs": "^8.0.0",
    "cheerio": "^1.0.0-rc.12",
    "crawlee": "^3.1.0",
    "googleapis": "^108.0.1",
    "html-to-text": "^8.2.1",
    "jsdom": "^20.0.1",
    "listr2": "^5.0.5",
    "playwright": "^1.27.1",
    "readability-scores": "^1.0.8",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.18.12/xlsx-0.18.12.tgz",
    "xpath-ts": "^1.3.13"
  }
}
