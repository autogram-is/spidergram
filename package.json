{
  "name": "spidergram",
  "version": "0.5.0",
  "description": "Structural analysis tools for complex web sites",
  "exports": "./dist/source/index.js",
  "types": "./dist/source/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=14.20.0"
  },
  "scripts": {
    "clean": "shx rm -rf ./dist; ava reset-cache",
    "nuke": "npm run clean; rm -rf ./node_modules; rm package-lock.json; npm install",
    "test": "ava",
    "lint": "xo",
    "fix": "xo --fix",
    "compile": "tsc",
    "prepare": "run-s clean compile",
    "pretest": "run-s clean compile",
    "posttest": "npm run lint",
    "scratch": "npm run clean; npm run compile; node dist/tests/fixtures/scratch.js"
  },
  "files": [
    "dist/source/**/*"
  ],
  "xo": {
    "space": true,
    "prettier": true,
    "bracketSpacing": true,
    "rules": {
      "new-cap": "off",
      "max-params": "off"
    }
  },
  "prettier": {
    "editorconfig": true,
    "semi": true,
    "singleQuote": true,
    "indent_style": "space",
    "indent_size": 2,
    "bracketSpacing": true
  },
  "ava": {
    "files": [
      "tests/**/*",
      "!tests/fixtures"
    ],
    "typescript": {
      "rewritePaths": {
        "tests/": "dist/tests/"
      },
      "compile": false
    }
  },
  "author": "Jeff Eaton <jeff@autogram.is> (http://eaton.fyi)",
  "license": "MIT",
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/app-root-path": "^1.2.4",
    "@types/cheerio": "^0.22.31",
    "@types/node": "^14.11.2",
    "@types/object-hash": "^2.2.1",
    "@types/uuid": "^8.3.4",
    "@types/whatwg-mimetype": "^2.1.1",
    "ava": "^4.3.1",
    "concurrently": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "typescript": "^4.7.4",
    "xo": "^0.51.0"
  },
  "dependencies": {
    "@autogram/autograph": "github:autogram-is/autograph#main",
    "@autogram/url-tools": "github:autogram-is/url-tools#main",
    "@sindresorhus/is": "^5.3.0",
    "@types/mime": "^3.0.1",
    "@types/mkdirp": "^1.0.2",
    "cheerio": "^1.0.0-rc.12",
    "cross-fetch": "^3.1.5",
    "eventemitter3": "^4.0.7",
    "fingerprint-generator": "^2.0.2",
    "got-scraping": "^3.2.9",
    "header-generator": "^2.0.3",
    "mime": "^3.0.0",
    "mkdirp": "^1.0.4",
    "ow": "^1.0.0",
    "p-queue": "^7.3.0",
    "type-fest": "^2.17.0",
    "whatwg-mimetype": "^3.0.0"
  }
}
