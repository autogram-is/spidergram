{
  "testing": true,
  "debug": false,
  "logLevel": "error",
  "storageDirectory": "/Users/jeff/Work/spidergram/storage",
  "arango": { "databaseName": "spidergram" },
  "crawlee": {},
  "spider": {
    "preNavigationHooks": [],
    "postNavigationHooks": [],
    "requestHandlers": {},
    "urlOptions": {
      "limit": 1000000,
      "selector": "body a",
      "save": "all",
      "enqueue": "same-domain",
      "prioritize": false,
      "checkRobots": false,
      "respectRobots": false,
      "checkSitemaps": false,
      "prioritizeSitemaps": false,
      "discardEmptyLinks": true,
      "discardAnchorOnlyLinks": true,
      "discardNonWebLinks": false,
      "discardUnparsableLinks": false,
      "discardExistingLinks": true
    },
    "parseMimeTypes": [ "text/html" ],
    "downloadMimeTypes": [],
    "userAgent": "Spidergram Tests",
    "handlerTimeout": 180
  },
  "htmlToText": {},
  "queries": {},
  "reports": {},
  "pageData": {
    "attributes": true,
    "head": true,
    "meta": true,
    "links": false,
    "noscript": false,
    "scripts": false,
    "styles": false,
    "templates": false
  },
  "pageContent": {
    "readability": true,
    "allowMultipleContentElements": false,
    "defaultToFullDocument": true,
    "trim": true
  },
  "urlNormalizer": {
    "forceProtocol": "https:",
    "forceLowercase": "host",
    "discardSubdomain": "ww*",
    "discardAnchor": true,
    "discardAuth": true,
    "discardIndex": "**/{index,default}.{htm,html,aspx,php}",
    "discardSearch": "!{page,p}",
    "discardTrailingSlash": false,
    "sortSearchParams": true
  }
}